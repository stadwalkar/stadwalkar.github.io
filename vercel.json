{
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    { "src": "/real-estate", "dest": "/real-estate.html" },
    { "src": "/startups", "dest": "/startups.html" },
    { "src": "/connect", "dest": "/connect.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
} 